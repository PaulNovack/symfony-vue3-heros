{
  "name": "heros-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite & vite build --watch",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "check-format": "prettier --check 'vue/**/*.{js,ts,vue}'",
    "format-code": "prettier --write 'vue/**/*.{js,ts,vue}'",
    "lint": "eslint 'vue/**/*.{js,ts,vue}'",
    "lint-fix": "eslint --fix 'vue/**/*.{js,ts,vue}'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "cypress run --spec \"cypress/e2e/HeroAppTest.cy.ts\"",
    "cy:component": "cypress open --component",
    "what": "node select-script.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "axios": "^1.7.7",
    "lodash": "^4.17.21",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@tailwindcss/postcss": "^0.0.0-development.1",
    "@types/cypress": "^0.1.6",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.14.2",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "inquirer": "^11.0.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.12",
    "typescript": "5.5.*",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.1",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^2.0.29"
  }
}
